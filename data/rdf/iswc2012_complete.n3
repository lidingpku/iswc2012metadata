      swrc:listKeyword "OWL Ontologies,Reasoner Performance Variability,Approximate Reasoning,NCBO BioPortal,Description Logics" ;
      swrc:pdf <http://iswc2012.semanticweb.org/sites/default/files/76490081.pdf> ;
