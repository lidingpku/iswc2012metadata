      swrc:listAuthor "Montiago Labute, Archana Venbakam, Sean Felten and Venkat Krishnamurthy" ;
      swrc:listKeyword "RDF,SPARQL,sentiment,graph,patterns,NLP" ;
      foaf:maker person:archana-venbakam , person:montiago-labute , person:venkat-krishnamurthy , person:sean-felten .

