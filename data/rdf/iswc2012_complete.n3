      swc:relatedToEvent iswc2012track:doctoral-consortium ;
      dc:creator person:enrico-daga ;
      dc:identifier "76500434" ;
      dc:subject "Linked Data" , " Data Harmonization" , " Planning" ;
      dc:title "Towards a theoretical foundation for the harmonization of linked data" ;
