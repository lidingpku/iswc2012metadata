      swrc:listKeyword "Collaborative filtering,Taxonomy of items,Recommendation over temporal dynamics,Time weight collaborative filtering" ;
      swrc:pdf <http://iswc2012.semanticweb.org/sites/default/files/76490353.pdf> ;
