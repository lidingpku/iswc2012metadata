  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2012/paper/research-14">
    <dc:creator rdf:resource="http://data.semanticweb.org/person/matthew-rowe"/>
    <dcterms:creator rdf:resource="http://data.semanticweb.org/person/matthew-rowe"/>
