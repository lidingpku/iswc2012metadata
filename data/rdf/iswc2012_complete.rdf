    <dcterms:title>MORe: Modular Combination of OWL Reasoners for Ontology Classification</dcterms:title>
    <swrc:author rdf:resource="http://data.semanticweb.org/person/bernardo-cuenca-grau"/>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/ana-armas"/>
