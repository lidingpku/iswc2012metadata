    <rdfs:label>A Comparison of Hard Filters and Soft Evidence for Answer Typing in Watson</rdfs:label>
    <dc:title>A Comparison of Hard Filters and Soft Evidence for Answer Typing in Watson</dc:title>
    <dcterms:creator rdf:resource="http://data.semanticweb.org/person/james-fan"/>
    <dc:identifier>76500240</dc:identifier>
    <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2012/proceedings"/>
