  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2012/paper/inuse-72">
    <dc:title>Incorporating Semantic Knowledge into Dynamic Data Processing for Smart Power Grids</dc:title>
    <dcterms:creator rdf:resource="http://data.semanticweb.org/person/viktor-prasanna"/>
    <rdfs:label>Incorporating Semantic Knowledge into Dynamic Data Processing for Smart Power Grids</rdfs:label>
    <bibo:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2012/paper/inuse-72/authors"/>
    <dcterms:title>Incorporating Semantic Knowledge into Dynamic Data Processing for Smart Power Grids</dcterms:title>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/qunzhi-zhou"/>
    <dc:subject>Semantic Web</dc:subject>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/viktor-prasanna"/>
    <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
