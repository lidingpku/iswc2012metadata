    <rdfs:label>Creating Enriched YouTube Media Fragments With NERD Using Timed-Text</rdfs:label>
    <swrc:author rdf:resource="http://data.semanticweb.org/person/giuseppe-rizzo"/>
    <dc:subject>NER</dc:subject>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/giuseppe-rizzo"/>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/raphael-troncy"/>
