    <dc:subject>interactive ontology debugging</dc:subject>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/gerhard-friedrich"/>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/patrick-rodler"/>
