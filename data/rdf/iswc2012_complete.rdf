  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2012/paper/research-07">
    <dcterms:title>Semantic Enrichment by Non-Experts: Usability of Manual Annotation Tools</dcterms:title>
    <swrc:author rdf:resource="http://data.semanticweb.org/person/annika-hinze"/>
