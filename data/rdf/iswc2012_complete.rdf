    <swrc:author rdf:resource="http://data.semanticweb.org/person/jacopo-urbani"/>
    <swrc:listAuthor>Spyros Kotoulas, Jacopo Urbani, Peter Boncz and Peter Mika</swrc:listAuthor>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/jacopo-urbani"/>
    <bibo:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2012/paper/research-31/authors"/>
