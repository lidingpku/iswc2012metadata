PREFIX foaf:   <http://xmlns.com/foaf/0.1/>
PREFIX rdfs:   <http://www.w3.org/2000/01/rdf-schema#>
PREFIX swrc:   <http://swrc.ontoware.org/ontology#>
select distinct ?s ?personLabel ?affiliationLabel  
where {
?s a foaf:Person.
?s rdfs:label ?personLabel .
OPTIONAL{ 
	?s swrc:affiliation ?affiliation .
	?affiliation rdfs:label ?affiliationLabel .
}
OPTIONAL{ ?s foaf:homepage ?homepage . }
OPTIONAL{ ?s foaf:firstName ?firstName. 
		  ?s foaf:family_name ?lastName. }

}
order by ?s